{
  "name": "@legionsafe/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for interacting with LegionSafe smart contracts",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "legionsafe",
    "vault",
    "defi",
    "trading",
    "automation",
    "ethereum",
    "bsc"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2",
    "viem": "^2.21.54",
    "vitest": "^1.0.0"
  }
}
