# Example environment variables for LegionSafe deployment and interaction
# Copy this file to env.sh and fill in your actual values
# IMPORTANT: env.sh is in .gitignore - never commit it with real credentials

# Deployer's private key (without 0x prefix)
export PRIVATE_KEY=your_private_key_here

# RPC URL for your target network
# Examples:
# - Base Sepolia: https://base-sepolia.g.alchemy.com/v2/YOUR_API_KEY
# - Ethereum Mainnet: https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY
# - Base Mainnet: https://base-mainnet.g.alchemy.com/v2/YOUR_API_KEY
export RPC_URL=https://base-sepolia.g.alchemy.com/v2/YOUR_API_KEY

# Deployed LegionSafe contract address (fill in after deployment)
export LEGION_SAFE_ADDRESS=0x0000000000000000000000000000000000000000

# Optional: Block explorer verification
export VERIFIER_URL=https://api-sepolia.basescan.org/api
export ETHERSCAN_API_KEY=your_etherscan_api_key

# Optional: Contract addresses for interactions
export DEX_ADDRESS=0x0000000000000000000000000000000000000000
export TOKEN_A_ADDRESS=0x0000000000000000000000000000000000000000
export TOKEN_B_ADDRESS=0x0000000000000000000000000000000000000000

# Optional: Owner and Operator addresses (for deployment)
export OWNER_ADDRESS=0x0000000000000000000000000000000000000000
export OPERATOR_ADDRESS=0x0000000000000000000000000000000000000000
